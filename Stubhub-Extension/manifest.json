{
    "name": "Ticket Gadgets Stubhub",
    "description": "Stubhub counter to quickly see sold and active listings, must use on a Stubhub event page.",
    "version": "1.1.0",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "32": "icon32.png",
        "16": "icon16.png"

    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon128.png"
    },
    "permissions": ["cookies",
        "*://*.livenation.com/event/*",
        "*://*.livenation.ca/event/*",
        "*://*.livenation.com/*/event/*",
        "*://*.livenation.ca/*/event/*",
        "*://*.ticketmaster.com/*/event/*",
        "*://*.ticketmaster.com/event/*",
        "*://*.ticketmaster.ca/*/event/*",
        "*://*.ticketmaster.ca/event/*",
        "*://my.ticketmaster.com/*",
        "*://my.ticketmaster.ca/*",
        "*://my.livenation.com/*",
        "*://my.livenation.ca/*",
        "https://*.ticketweb.com/*",
        "https://*.ticketweb.ca/*",
        "https://*.stubhub.com/*",
        "storage",
        "http://*/*",
        "https://*/*",
        "activeTab",
        "tabs",
        "*://ticketgadgets.com/",
        "*://www.ticketgadgets.com/",
        "<all_urls>"
    ],
    "manifest_version": 2,
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens popup.html"
        }
    }
}